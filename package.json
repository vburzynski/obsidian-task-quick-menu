    "builtin-modules": "4.0.0",
    "esbuild": "0.24.0",
    "eslint": "^9.15.0",
    "globals": "^15.12.0",
    "husky": "^9.1.7",
    "obsidian": "^1.7.2",
    "tslib": "2.8.1",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.15.0"
  }
